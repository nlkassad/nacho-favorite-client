<h3>Menu Item: {{menu_item.dish_id}} Restaurant: {{menu_item.restaurant_id}}</h3>

{{#if rating}}
  <p>Rating: {{rating}}</p>
{{else}}
  <p> No rating </p>
{{/if}}

{{#if note}}
  <p>Notes: {{note}}</p>
{{else}}
  <p> No notes </p>
{{/if}}
<button id="editReviewButton" name={{id}} class="btn btn-default">Edit Review</button>
<button id="deleteReviewButton" name={{id}} class="btn btn-default">Delete Review</button>
